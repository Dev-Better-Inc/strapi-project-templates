{
  "name": "@app/cms",
  "private": true,
  "description": "CMS & API provided by Strapi",
  "scripts": {
    "config-sync": "node_modules/.bin/config-sync",
    "config:export": "yarn config-sync export --yes",
    "config:import": "yarn config-sync import --yes",
    "start": "yarn env && strapi develop",
    "generate": "yarn strapi ts:generate-types --file=src/types/api.d.ts",
    "serve": "yarn strapi start",
    "env": "yarn vault-env",
    "prebuild": "yarn env && yarn config:export",
    "build": "yarn strapi build"
  },
  "eslintConfig": {
    "extends": "@bn-digital/eslint-config/typescript",
    "overrides": [
      {
        "files": [
          "**/*.d.ts"
        ],
        "rules": {
          "@typescript-eslint/ban-types": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@strapi/database": "^4.3.0",
    "@strapi/plugin-content-manager": "^4.3.0",
    "@strapi/plugin-content-type-builder": "^4.3.0",
    "@strapi/plugin-graphql": "^4.3.0",
    "@strapi/plugin-i18n": "^4.3.0",
    "@strapi/plugin-seo": "^1.7.4",
    "@strapi/plugin-users-permissions": "^4.3.0",
    "@strapi/provider-email-nodemailer": "^4.3.0",
    "@strapi/provider-upload-aws-s3": "^4.3.0",
    "@strapi/strapi": "^4.3.0",
    "@strapi/typescript-utils": "^4.3.0",
    "better-sqlite3": "^7.6.2",
    "knex": "^2.2.0",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "sqlite3": "^5.0.10",
    "strapi-plugin-config-sync": "^1.0.2",
    "strapi-plugin-email-designer": "^2.1.1",
    "strapi-plugin-import-export-entries": "^1.6.9",
    "strapi-plugin-menus": "^0.3.3",
    "strapi-plugin-sitemap": "^2.0.8",
    "strapi-plugin-wysiwyg-tui-editor": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "strapi": {
    "id": "fdaea281-8950-4f22-a7cf-8d27600eb545"
  },
  "devDependencies": {
    "@bn-digital/eslint-config": "^1.20.4",
    "@bn-digital/vault-env": "^1.4.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/node": "^18.6.2",
    "@types/uuid": "^8.3.4",
    "nexus": "^1.3.0",
    "typescript": "^4.7.4"
  }
}
