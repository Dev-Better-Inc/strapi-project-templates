{
  "name": "@app/cms",
  "private": true,
  "description": "CMS & API provided by Strapi",
  "scripts": {
    "config-sync": "node_modules/.bin/config-sync",
    "config:export": "yarn config-sync export --yes",
    "config:import": "yarn config-sync import --yes",
    "start": "yarn env && yarn strapi develop",
    "generate": "yarn strapi ts:generate-types --file=src/types/api.d.ts",
    "serve": "yarn strapi start",
    "env": "yarn vault-env",
    "prebuild": "yarn config:export",
    "build": "yarn strapi build"
  },
  "strapi": {
    "id": "68be1d7d-5510-45e4-a7fb-a93f6bb691f2"
  },
  "eslintConfig": {
    "extends": "@bn-digital/eslint-config/typescript"
  },
  "dependencies": {
    "@strapi/plugin-content-manager": "4.3.2",
    "@strapi/plugin-content-type-builder": "4.3.2",
    "@strapi/plugin-graphql": "4.3.2",
    "@strapi/plugin-i18n": "4.3.2",
    "@strapi/plugin-seo": "^1.7.4",
    "@strapi/plugin-users-permissions": "4.3.2",
    "@strapi/provider-email-nodemailer": "4.3.2",
    "@strapi/provider-upload-aws-s3": "4.3.2",
    "@strapi/strapi": "4.3.2",
    "@strapi/typescript-utils": "4.3.2",
    "better-sqlite3": "^7.6.2",
    "knex": "^2.2.0",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "strapi-plugin-config-sync": "^1.0.3",
    "strapi-plugin-email-designer": "^2.1.1",
    "strapi-plugin-import-export-entries": "^1.12.0",
    "strapi-plugin-menus": "^1.0.2",
    "strapi-plugin-sitemap": "^2.0.8",
    "strapi-plugin-wysiwyg-tui-editor": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@bn-digital/vault-env": "^1.4.0",
    "@types/uuid": "^8.3.4",
    "nexus": "^1.3.0",
    "typescript": "^4.7.4"
  }
}
