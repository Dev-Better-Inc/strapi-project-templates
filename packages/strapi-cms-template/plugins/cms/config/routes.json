{
  "routes": [
    {
      "method": "GET",
      "path": "/websites/domain/:domain",
      "handler": "cms.website.findOneByDomain",
      "config": {
        "policies": [
          "plugins::users-permissions.ratelimit"
        ]
      }
    },
    {
      "method": "GET",
      "path": "/categories",
      "handler": "cms.category.find",
      "config": {
        "policies": [
          "plugins::users-permissions.ratelimit"
        ]
      }
    },
    {
      "method": "GET",
