{
  "name": "@project-templates/website",
  "private": true,
  "scripts": {
    "vite": "node_modules/.bin/vite",
    "env": "yarn vault-env",
    "build": "yarn vite build",
    "postbuild": "react-snap",
    "test": "cypress open --e2e --browser=chrome",
    "serve": "yarn dlx serve --single build",
    "start": "yarn vite serve"
  },
  "eslintConfig": {
    "extends": "@bn-digital/eslint-config/react"
  },
  "proxy": "http://localhost:1337",
  "reactSnap": {
    "inlineCss": true,
    "cacheAjaxRequests": true,
    "navigateFallback": "index.html",
    "skipThirdPartyRequests": true
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "antd": "^4.22.8",
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.1.0",
    "react-markdown": "^8.0.3",
    "react-responsive": "^9.0.0-beta.10",
    "react-router": "^6.4.0-pre.13",
    "react-router-dom": "^6.4.0-pre.13",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@bn-digital/sdk": "^1.3.0",
    "@bn-digital/vault-env": "^1.4.0",
    "@bn-digital/vite": "^2.0.4",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "cypress": "^10.7.0",
    "dotenv": "^16.0.2",
    "puppeteer": "^17.0.0",
    "puppeteer-core": "^17.0.0",
    "react-snap": "^1.23.0",
    "typescript": "^4.8.2"
  }
}
